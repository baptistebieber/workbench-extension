{
    "manifest_version": 2, 
    "name": "Workbench Extension",
    "version": "1.0.0",
    "description": "Add functionalities to Workbench!",
    "icons": {
        "16": "img/icon-16.png",
        "32": "img/icon-32.png",
        "48": "img/icon-48.png",
        "128": "img/icon-128.png"
    },
    "browser_action": {
        "default_icon": "img/icon-16.png"
    },
    "content_scripts" : [{
        "matches" : ["https://workbench.developerforce.com/*"], 
        "css" : ["css/style.css"], 
        "js" : ["js/jquery.min.js","js/script.js"] 
    }],
    "permissions": [ 
        "tabs", "https://workbench.developerforce.com/*" 
    ]
}
